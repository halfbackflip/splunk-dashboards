  "dataSources": {
    "ds_search_2": {
      "type": "ds.search",
      "options": {
        "query": "| inputlookup log_sources_os_win.csv\n| eval sourcetype=split(sourcetype, \" \")\n| search index IN (*) AND sourcetype IN (*) AND host IN (*)\n| sort + host\n| lookup index_description.csv index AS index OUTPUT index_description\n| lookup sourcetype_description.csv sourcetype AS sourcetype OUTPUT sourcetype_description\n| fillnull value=0\n| fields + host, index, sourcetype, index_description sourcetype_description",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now"
        }
      },
      "name": "log_sources_win_unix_v5"
    },
    "ds_uakEQ86o": {
      "type": "ds.search",
      "options": {
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now"
        },
        "query": "| inputlookup log_sources.csv\n| eval sourcetype=split(sourcetype, \" \")\n| search (index IN ($_index$) AND sourcetype IN ($_sourcetype$))\n| lookup index_description.csv index AS index OUTPUT index_description\n| lookup sourcetype_description.csv sourcetype AS sourcetype OUTPUT sourcetype_description\n| fillnull value=0\n| eval sourcetype_definition = mvzip(sourcetype, sourcetype_description, \" : \")\n| fields + consumer index index_description sourcetype sourcetype_description sourcetype_definition"
      },
      "name": "main_base_search_v5"
    },
    "ds_FVahHMTR": {
      "type": "ds.chain",
      "options": {
        "extend": "ds_search_2",
        "query": "| table host, index, sourcetype sourcetype_description"
      },
      "name": "win_unix_logs_v5"
    },
    "ds_aIFoVcza": {
      "type": "ds.search",
      "options": {
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now"
        },
        "query": "| inputlookup log_sources.csv\n| fields index\n| table index\n| eval index_escape=\"\\\"\".index.\"\\\"\"\n| sort - index"
      },
      "name": "index_multiselect_v5"
    },
    "ds_FWrxyA3P_ds_xmQcjN7F": {
      "type": "ds.chain",
      "options": {
        "extend": "ds_uakEQ86o",
        "query": "| table index index_description sourcetype sourcetype_description sourcetype_definition \n| stats values(index_description) as index_description values(sourcetype) as sourcetype values(sourcetype_definition) as sourcetype_definition by index"
      },
      "name": "log_sourcetypes_ex_win_unix_v5"
    },
    "ds_x7iC3LRn": {
      "type": "ds.search",
      "options": {
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now"
        },
        "query": "| inputlookup log_sources.csv\n| search index != \"os\"\n| eval sourcetype=split(sourcetype, \" \") \n| mvexpand sourcetype\n| fields sourcetype\n| table sourcetype\n| dedup sourcetype\n| eval sourcetype_escape=\"\\\"\".sourcetype.\"\\\"\"\n| sort - sourcetype"
      },
      "name": "sourcetype_multiselect_v5"
    },
    "ds_Zm4nbhxY": {
      "type": "ds.search",
      "options": {
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now"
        },
        "query": "| inputlookup dm_summary.csv\n| eval sourcetype=split(dm_sourcetype, \" \") \n| eval index=split(dm_index, \" \")\n| search (index IN ($_index$) AND sourcetype IN ($_sourcetype$))\n| table modelName, index, sourcetype"
      },
      "name": "dm_search_v5"
    },
    "ds_ZtKqHNR8_ds_Zm4nbhxY": {
      "type": "ds.search",
      "options": {
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now"
        },
        "query": "| inputlookup dm_summary.csv\r\n| eval sourcetype=split(dm_sourcetype, \" \") \r\n| eval index=split(dm_index, \" \")\r\n| search (index IN ($_index$) AND sourcetype IN ($_sourcetype$))\r\n| chart count by modelName, sourcetype limit=0"
      },
      "name": "dm_chart_v5"
    }
  },
